{
  "name": "@marreman/elm-i18next",
  "version": "0.0.1",
  "description": "Generate Elm modules from i18n translation JSON files.",
  "bin": {
    "elm-i18next": "bin/elm-i18next"
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "prepare": "elm-tooling install",
    "test": "elm-test",
    "build": "elm make src/Program.elm --output=bin/program.js --optimize",
    "generate-examples": "examples/helloWorld/generate && examples/multiLevel/generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marreman/elm-i18next.git"
  },
  "keywords": [
    "Elm",
    "translations",
    "i18n",
    "generation"
  ],
  "author": "Martin Liberg <mr.liberg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marreman/elm-i18next/issues"
  },
  "homepage": "https://github.com/marreman/elm-i18next#readme",
  "files": [
    "bin/elm-i18next",
    "bin/program.js"
  ],
  "devDependencies": {
    "elm-test": "^0.19.1-revision6",
    "elm-tooling": "^1.2.0"
  }
}
